{
  "name": "backchannel",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.tsx",
  "scripts": {
    "start": "react-scripts start",
    "build": "cd node_modules/automerge && npm i && npm run build && cd ../.. && react-scripts build",
    "test": "react-scripts test",
    "deploy": "npm run build && npm run build:docs",
    "relay": "DEBUG=* PORT=3001 node ./node_modules/@localfirst/relay/dist/start.js",
    "lint": "prettier -c src",
    "format": "prettier -w src",
    "build:docs": "typedoc src/ --excludeExternals --excludePrivate --excludeInternal --out build/docs/api --readme docs/readme.md"
  },
