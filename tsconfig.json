{
	"include": [
		"src/**/*"
	],
	"compilerOptions": {
		"noImplicitAny": false,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"noFallthroughCasesInSwitch": true,
		"allowJs": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"target": "es5",
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
	}
	
}